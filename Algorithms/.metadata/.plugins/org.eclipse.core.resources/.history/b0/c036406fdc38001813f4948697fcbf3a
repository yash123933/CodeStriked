	public static int myAtoi(String str) {
        int value;
        if(str.charAt(0)=='-')
            value=0;
        else
            value=1;
        int ans=0;
        if(value==1)
            for(int i=0;i<str.length();i++)
                ans=(int) (ans+((str.charAt(i)-'0')*Math.pow(10,str.length()-i-1)));
        else
            for(int i=1;i<str.length();i++)
                ans=(int) (ans+((str.charAt(i)-'0')*Math.pow(10,str.length()-i-1)));
		return ans;
        
    }

}
