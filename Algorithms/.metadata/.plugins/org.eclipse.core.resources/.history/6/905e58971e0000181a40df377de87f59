
public class TwoKeys {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=21,sum=0;
		while(n!=1){
			if(n%2==0){
				n=n/2;
				sum+=2;
			}
			else if(n%3==0){
				n=n/3;
				sum+=3;
			}
			else if(n%5==0){
				n=n/5;
				sum+=5;
			}
			else{
				n=n/7;
				sum+=7;
			}
		}
		System.out.println(sum);
	}

}
